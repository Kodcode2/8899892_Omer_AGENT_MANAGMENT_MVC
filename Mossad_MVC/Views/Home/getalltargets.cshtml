@model IEnumerable<Mossad_MVC.Models.Target>

@{
    ViewData["Title"] = "getalltargets";
}

<h1>getalltargets</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Position)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PhotoUrl)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.X_axis)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Y_axis)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Eliminated)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Position)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PhotoUrl)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.X_axis)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Y_axis)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Eliminated)
            </td>
        </tr>
}
    </tbody>
</table>
